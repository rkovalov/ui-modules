{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "react-app",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "noEmit": true,
    "target": "es5"
  }
}
