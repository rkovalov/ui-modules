{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "storybook build --docs --output-dir dist",
    "build:by:rsbuild:experimental": "COMPILER=rsbuild storybook build --docs --output-dir dist",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "dev": "storybook dev -p 6006",
    "preview-storybook": "serve storybook-static"
  },
  "dependencies": {
    "@ui-modules/themes": "workspace:*",
    "@ui-modules/ui": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.3.4",
    "@storybook/addon-designs": "^8.0.3",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/addon-links": "^8.3.4",
    "@storybook/blocks": "^8.3.4",
    "@storybook/react": "^8.3.4",
    "@storybook/react-vite": "^8.3.4",
    "@types/node": "^22.7.4",
    "@ui-modules/typescript-config": "workspace:*",
    "@vitejs/plugin-react": "^4.3.1",
    "serve": "^14.2.3",
    "storybook": "^8.3.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "@rsbuild/core": "^1.0.7",
    "@rsbuild/plugin-react": "^1.0.2",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "storybook-react-rsbuild": "^0.1.0"
  }
